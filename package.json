{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate with webpack 5 for webdev",
  "private": "true",
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "scripts": {
    "build-dev": "webpack --config configuration/webpack.dev.config.js --mode=development",
    "build-prod": "webpack --config configuration/webpack.prod.config.js --mode=production",
    "start": "webpack serve --config configuration/webpack.dev.config.js --mode=development",
    "predeploy": "npm run build-prod",
    "deploy-gh": "gh-pages -b deployment -d dist"
  },
  "keywords": [
    "webpack",
    "template",
    "boilerplate"
  ],
  "author": "Ilya Dokuchaev <miamorokor@mail.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ilya-Dokuchaev/webpack-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/Ilya-Dokuchaev/webpack-boilerplate/issues",
    "email": "miamorokor@mail.ru"
  },
  "homepage": "https://github.com/Ilya-Dokuchaev/webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@squoosh/lib": "^0.5.3",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "gh-pages": "^5.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "sass": "^1.63.6",
    "sass-lint": "^1.13.1",
    "sass-loader": "^13.3.2",
    "template-ejs-loader": "^0.9.4",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
